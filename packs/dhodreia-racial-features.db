{"name":"Darkvision","type":"feat","img":"icons/magic/perception/eye-ringed-glow-angry-small-teal.webp","system":{"description":{"value":"<p><strong>Darkvision. </strong>You can see in dim light within 60 feet of you as if it were bright light and in darkness as if it were dim light. You discern colors in that darkness only as shades of gray.</p>","chat":"","unidentified":""},"source":"Dho'Dreia","activation":{"type":"","cost":null,"condition":""},"duration":{"value":"","units":""},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":null,"recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":"","attackBonus":"","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"type":{"value":"race","subtype":""},"requirements":"","recharge":{"value":null,"charged":false}},"effects":[{"label":"Darkvision","icon":"icons/magic/perception/eye-ringed-glow-angry-small-teal.webp","origin":"Item.QVXdzy5bJHj7LbS0","duration":{"startTime":null,"seconds":null,"combat":null,"rounds":null,"turns":null,"startRound":null,"startTurn":null},"disabled":false,"_id":"KvUkCzG5jniIEcxD","changes":[{"key":"system.attributes.senses.darkvision","mode":5,"value":"60","priority":null}],"tint":null,"transfer":true,"flags":{}}],"flags":{"rsr5e":{"quickDesc":{"value":true},"quickFooter":{"value":true}},"core":{"sourceId":"Item.QVXdzy5bJHj7LbS0"}},"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1681523593719,"modifiedTime":1681614927954,"lastModifiedBy":"tl7rQdo93TVsRvdn"},"folder":null,"sort":0,"ownership":{"default":0,"tl7rQdo93TVsRvdn":3},"_id":"qRgjOwROqyhrncKy"}
